<div class="page-header">
  <h1><%= t("term.detail") %></h1>
</div>

<div>
  <%= @term.title%>
</div>

<div>
  <h3>ENG</h3>
  <%=simple_format @term.eng %>
</div>

<div>
  <h3>ITA</h3>
  <%=simple_format @term.ita %>
</div>

<div>
