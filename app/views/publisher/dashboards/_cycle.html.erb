<div class="cycle">
	<div class="pane">
		<h3><%= t("publisher.home.slide1")%></h3>
	</div>
	<div class="pane">
		<h3><%= t("publisher.home.slide2")%></h3>
	</div>
</div>

<script type="text/javascript">
$.fn.cycle.defaults.speed   = 900;
$.fn.cycle.defaults.timeout = 6000;

$(function() {
	$('.cycle')
		.after('<div id="navcycle">')
		.cycle({ 
	    fx:      'scrollHorz', 
	    speed:    1000,
	    pause:		1,
	    easeOut: 	'easeInOutCubic',
	    easeIn: 	'easeInOutCubic',
	    pager:  	'#navcycle', 
	    timeout:  10000 
	});
});
</script>
